  "scripts": {
    "re": "nvm use 21.5.0 && nf start",
    "start:react": "cross-env BROWSER=none PORT=5000 craco start",
    "start:electron": "cross-env PORT=5000 electron ./main.js",
    "waitStartElectron": "cross-env PORT=5000 node ./waitStartElectron.js",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "padding-line-between-statements": [
        "error",
        {
          "blankLine": "always",
          "prev": "const",
          "next": "return"
        },
        {
          "blankLine": "always",
          "prev": "let",
          "next": "return"
        },
        {
          "blankLine": "always",
          "prev": "block-like",
          "next": "block-like"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@craco/types": "^7.1.0",
    "@electron-forge/cli": "^7.4.0",
    "@stylexjs/babel-plugin": "^0.5.1",
    "@stylexjs/eslint-plugin": "^0.5.1",
    "@stylexjs/webpack-plugin": "^0.5.1",
    "@types/lodash": "^4.17.0",
    "@webgpu/types": "^0.1.40",
    "cross-env": "^7.0.3",
    "electron": "^29.1.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^3.0.1",
    "foreman": "^3.0.1"
  }
