{
  "manifest_version": 3,
  "name": "adamExtension",
  "short_name": "adamExtension",
  "version": "1.0.0.0",
  "version_name": "1.0.0",
  "description": "Listen for window change event",
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/setup.ts-loader-Dxh6YD90.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://*/*"
      ],
      "resources": [
        "assets/index-CGwCSIWv.js",
        "assets/setup.ts-Ds52jjOk.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
